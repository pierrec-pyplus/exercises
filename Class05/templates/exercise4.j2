{%- for vrf in vrf_list %}
vrf definition {{ vrf["vrf_name"] }}
rd {{ vrf["rd_number"] }}
{%- if vrf["ipv4"] %}
!
address-family ipv4
  route-target export {{ vrf["rd_number"] }}
  route-target import {{ vrf["rd_number"] }}
exit-address-family
{%- endif %}
{%- if vrf["ipv6"] %}
!
address-family ipv6
  route-target export {{ vrf["rd_number"] }}
  route-target import {{ vrf["rd_number"] }}
exit-address-family
{%- endif %}
!
!
{%- endfor %}
